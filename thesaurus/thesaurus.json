                }
            ],
            "scopeNote": {
                "@language": "nl",
                "@value": "Bron definitie: https://www.amnesty.nl/wat-we-doen/demonstratierecht-in-nederland/wat-is-geen-demonstratie"
            }
        },
        {
            "@id": "https://digitaalerfgoed.poolparty.biz/atm/450b0fa5-cb70-423f-93c4-7d36f1b9a56f",
            "@type": "Concept",
