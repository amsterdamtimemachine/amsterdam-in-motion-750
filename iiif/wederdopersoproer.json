    {
      "id": "https://amsterdamtimemachine.github.io/amsterdam-in-motion-750/iiif/wederdopersoproer.json/p1/canvas/6",
      "type": "Canvas",
      "label": {
        "nl": [
          "Nacht-vergaderingen der Adamieten"
        ]
      },
      "height": 1812,
      "width": 2483,
      "metadata": [
        {
          "label": {
            "nl": [
